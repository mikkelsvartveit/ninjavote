{
  "name": "voterush-backend",
  "version": "0.0.1",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run compile && npm run mocha",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "dev": "ts-node-dev --no-notify src/",
    "start": "node lib/",
    "build:frontend": "cd ../frontend && npm ci && npm run build && cd ../backend && rm -rf public && mkdir public && cp -r ../frontend/build/* public",
    "mocha": "mocha --require ts-node/register --require source-map-support/register \"test/**/*.ts\" --recursive --exit",
    "compile": "shx rm -rf lib/ && tsc"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/configuration": "^4.5.15",
    "@feathersjs/errors": "^4.5.15",
    "@feathersjs/express": "^4.5.15",
    "@feathersjs/feathers": "^4.5.15",
    "@feathersjs/socketio": "^4.5.15",
    "@feathersjs/transport-commons": "^4.5.15",
    "bcrypt": "^5.0.1",
    "better-sqlite3": "^7.5.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-hooks-common": "^6.1.0",
    "feathers-knex": "^8.0.1",
    "helmet": "^4.6.0",
    "knex": "^2.1.0",
    "mssql": "^9.0.1",
    "mysql": "^2.18.1",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^5.0.8",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/mocha": "^9.1.1",
    "@types/serve-favicon": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "axios": "^0.27.2",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "mocha": "^9.2.2",
    "shx": "^0.3.4",
    "ts-node-dev": "^2.0.0-0",
    "typescript": "^4.7.2"
  }
}
