{
  "name": "ninjavote-frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "install:all": "npm install && cd ../backend && npm install",
    "start": "concurrently -n Feathers,SvelteKit -c magenta,yellow \"cd ../backend && npm run dev\" \"cd ../frontend && npm run dev\"",
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^1.0.3",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/kit": "^1.15.5",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "prettier-plugin-svelte": "^2.10.0",
    "sass": "^1.62.0",
    "svelte": "^3.58.0",
    "svelte-check": "^2.10.3",
    "svelte-preprocess": "^5.0.3",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.2.1"
  },
  "dependencies": {
    "@feathersjs/client": "^4.5.16",
    "@feathersjs/feathers": "^4.5.16",
    "@feathersjs/socketio-client": "^4.5.16",
    "socket.io-client": "^2.5.0"
  },
  "type": "module"
}
